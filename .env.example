# ==================================
# Golden Retriever Platform - Environment Configuration
# ==================================

# Qdrant Cloud Configuration
QDRANT_API_KEY=your_qdrant_api_key_here
QDRANT_URL=https://your-cluster.gcp.cloud.qdrant.io:6333

# Transitland API
TRANSITLAND_API_KEY=your_transitland_api_key_here
TRANSITLAND_BASE_URL=https://transit.land/api/v2

# Service URLs
AI_SERVICE_URL=http://localhost:5001
DIGITAL_TWIN_URL=http://localhost:8000
BACKEND_URL=http://localhost:5000

# AI Service Integration (Digital Twin)
AI_SERVICE_ENABLED=true
AI_SERVICE_TIMEOUT=5

# Hugging Face (optional, for offline models)
HF_TOKEN=your_huggingface_token_here
